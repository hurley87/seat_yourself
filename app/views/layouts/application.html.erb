<!DOCTYPE html>
<html>
<head>
  <title>OpenBar</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

	<% if current_user %>
			<%= link_to "Log Out", logout_path %>
	<% else %>
		<h3><%= link_to "Create user", new_user_path %> | 	
		<%= link_to "Log In", new_session_path %></h3>
	<% end %>
<%= yield %>

</body>
</html>
